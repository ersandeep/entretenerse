<h1>Editing campaign_type</h1>

<% form_for(@campaign_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @campaign_type %> |
<%= link_to 'Back', campaign_types_path %>
